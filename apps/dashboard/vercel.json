{
  "version": 2,
  "public": false,
  "github": {
    "enabled": false
  },
  "regions": ["iad1"],
  "installCommand": "bun install --cwd ../..",
  "buildCommand": "cd ../.. && turbo build --filter=@midday/dashboard",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "headers": [
    {
      "source": "/_next/static/:path*",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://app.midday.ai"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}
